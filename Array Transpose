
public class ArrayTranspose {
    public static void main(String[] args) {
       
       int [][]arr = {{1,3,5,7},
                       {2,4,6,8},
                       {9,11,13,15},
                       {10,12,14,16}
                     };
       
      for( int i=0; i<arr.length; i++){
           for( int j=0; j<arr.length; j++){
         
            System.out.println(arr[j][i] + "\t");     
             
           }
          
       }
      
     }
      
 }
