
public class Array2D {
    public static void main(String[] args) {
       int [] [] a= { {1,2,3,4},
                      {2,5,3,4},
                      {6,8,7,6},
                      {8,9,7,3}
                    };  
      
      for( int i=0; i<a.length; i++){
           for( int j=0; j<a.length; j++){
          System.out.print(a[i][j]+ "\t");
    
      }
       
       }
    }
}                
