
public class  ArrayIntersection{
    public static void main(String[] args) {
       int[] arr1 ={3,5,8,10,18};
       int[] arr2 ={3,6,9,10,18};
       for ( int i = 0 ; i < arr1.length ; i++){
             for ( int j = 0 ; j < arr1.length ; j++){
                  if(arr1[i] == arr2[j]){
                    System.out.print(arr1[i] + " ");  
                    break;
            
                 }
            
            }
         
        }
   
    }
}
