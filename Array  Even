
public class Array{
  
 
      public static void main(String[] args) {
     
      int[]arr1={3,5,10,15,18,20,35,40,50,90};
      for(int i=0; i<arr1.length;i++){
          if( arr1[i]%2==0){
             System.out.println(arr1[i]+" is Even");
          
            }
    
        }   
   
     }
  }

